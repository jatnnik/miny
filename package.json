{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i app/styles/tailwind.css -o app/styles/tailwind-build.css",
    "typecheck": "tsc -b",
    "lint": "eslint --ext \".ts, .tsx\" --ignore-path .gitignore .",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.13",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@tailwindcss/forms": "^0.5.3",
    "@vercel/node": "^2.6.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.2.4",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.2",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.2",
    "framer-motion": "^7.5.2",
    "ics": "^2.40.0",
    "nanoid": "^4.0.0",
    "nodemailer": "^6.7.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.1.1",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^5.11.0",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/nodemailer": "^6.4.5",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": "22.x"
  },
  "sideEffects": false
